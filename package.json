{
  "name": "woku",
  "title": "woku",
  "description": "woku with nodejs",
  "main": "app.js",
  "homepage": "http://wokuer.com",
  "author": "ascoders (翱翔大空)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascoders/woku-node.git"
  },
  "keywords": [
    "woku"
  ],
  "bugs": {
    "url": "https://github.com/ascoders/woku-node/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ascoders/woku-node/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "co-body": "^4.0.0",
    "koa": "^1.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-conditional-get": "^1.0.3",
    "koa-etag": "^2.0.0",
    "koa-generic-session": "^1.9.2",
    "koa-redis": "^1.0.1",
    "koa-router": "^5.2.3",
    "koa-static": "^1.4.9",
    "koa-static-cache": "^3.1.2",
    "log4js": "^0.6.27",
    "mysql": "^2.9.0",
    "node-schedule": "^0.2.9",
    "nodemailer": "^1.6.0",
    "nodemailer-smtp-transport": "^1.0.3",
    "pm2": "^0.15.7",
    "redis": "^2.1.0",
    "sequelize": "^3.11.0",
    "validator": "^4.1.0"
  },
  "devDependencies": {
    "antd": "^0.9.1",
    "autoprefixer-loader": "^3.1.0",
    "babel-loader": "^5.3.2",
    "blanket": "^1.1.7",
    "co-mocha": "^1.1.2",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "history": "^1.12.1",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.3.3",
    "muk": "^0.4.0",
    "node-sass": "^3.3.3",
    "nodemon": "^1.7.1",
    "react": "^0.13.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0-rc1",
    "rewire": "^2.3.4",
    "sass-loader": "^3.0.0",
    "should": "^7.1.0",
    "style-loader": "^0.12.4",
    "superagent": "^1.4.0",
    "travis-cov": "^0.2.5",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "scripts": {
    "server": "nodemon --watch validate --watch server/controllers --watch server/lib  --watch server/models app.js",
    "webpack-dev": "node webpack.js",
    "webpack-dist": "webpack -p",
    "pm2": "pm2 start app.js",
    "pm2-clean": "pm2 delete app.js",
    "mocha-travis": "mocha --harmony -R travis-cov --recursive test",
    "mocha-html": "mocha --harmony -R html-cov > coverage.html --recursive test",
    "mocha": "mocha --harmony test --recursive -s 10 --check-leaks --es_staging",
    "start": "npm run server & npm run webpack-dev",
    "test": "npm run mocha-travis && npm run mocha-html && npm run mocha",
    "dist": "npm run webpack-dist && npm run pm2",
    "clear": "npm run pm2-clean"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib",
        "controllers",
        "models",
        "config"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "version": "1.0.0",
  "directories": {
    "test": "test"
  },
  "license": "ISC"
}
